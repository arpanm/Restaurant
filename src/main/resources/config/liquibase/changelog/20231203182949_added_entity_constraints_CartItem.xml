<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity CartItem.
    -->
    <changeSet id="20231203182949-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="item_id"
                                 baseTableName="cart_item"
                                 constraintName="fk_cart_item__item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="item"
                                 />

        <addForeignKeyConstraint baseColumnNames="meal_id"
                                 baseTableName="cart_item"
                                 constraintName="fk_cart_item__meal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="meal_plan"
                                 />

        <addForeignKeyConstraint baseColumnNames="cart_id"
                                 baseTableName="cart_item"
                                 constraintName="fk_cart_item__cart_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cart"
                                 />
    </changeSet>
</databaseChangeLog>
